{
  "name": "pixellint",
  "displayName": "pixellint",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": ["*"],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "linting",
      "properties": {
        "examMode.enabled": {
          "type": "boolean",
          "default": true,
          "description": "linting mode is active"
        }
      }
    }
  }
}
